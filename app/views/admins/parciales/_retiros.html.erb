<turbo-frame id="contenido">
  <%= form_with url: buscar_retiros_admins_path, method: :post, turbo_stream: true  do |form| %>
    <%= content_tag :div do %>
      <%= form.label :fe_ini, "Fecha inicial", class: "form-label" %>
      <%= form.date_field :fe_ini %>
    <% end %>
    <%= content_tag :div do %>
      <%= form.label :fe_fin, "Fecha final", class: "form-label" %>
      <%= form.date_field :fe_fin %>
    <% end %>
    <% submit "Buscar" %>
  <% end %>
  <%= turbo_frame_tag 'retirados' do %>
    <h1>RETIROS</>
  <% end %>
</turbo-frame>
