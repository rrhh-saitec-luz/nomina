<turbo-frame id="contenido">
  <% unless flash[:notice].eql?('') %>
    <div class="alert" style="color: green;"><%= flash[:notice] %></div>
  <% end %>
  <% unless flash[:alert].eql?('') %>
    <div class="alert" style="color: red;"><%= flash[:alert] %></div>
  <% end %>
  <%= form_with url: retirar_admins_path,
                method: :post,
                data: { turbo_stream: true },
                class: "d-flex flex-column gap-3 mt-3 mb-3" do |form| %>
    <%= content_tag :div do %>
      <%= form.label "Fecha Inicio", class: "form-label" %>
      <%= form.date_field :fe_inicio, required: true, class: "form-control" %>
    <% end %>
    <%= content_tag :div do %>
      <%= form.label "Fecha final", class: "form-label" %>
      <%= form.date_field :fe_fin,required: true, class: "form-control" %>
    <% end %>
    <%= content_tag :div, class: "row justify-content-center"  do %>
      <%= form.submit "Buscar", class: "col-2 align-self-center btn btn-primary" %>
    <% end %>
  <% end %>
  <%= turbo_frame_tag "retirados" do %>
    <h3 class="text-center">Resultados<h3/>
  <% end %>
</turbo-frame>
