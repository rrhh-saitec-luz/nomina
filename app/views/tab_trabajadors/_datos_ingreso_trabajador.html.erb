<div class="d-flex justify-content-center border border-bottom-2">
  <h5>Datos del ingreso</h5>
</div>
<ul class="list-group list-group-horizontal-md">
  <% @datos_ingreso.each do |key, val| %>
    <li class="list-group-item flex-fill">
      <div><b><p><%= key %></p></b></div>
      <div>
        <% if val.size.eql?(1) %>
          <%= val.first %>
        <% else %>
          <%= val.first %> - <%= val.last %>
        <% end %>
    </li>
  <% end %>
</ul>
